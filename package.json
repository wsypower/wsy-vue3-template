    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "shell:rm-git-merged": "./scripts/sh/git.rm.sh",
    "cz": "git-cz",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "esno scripts/release.js",
    "release-patch": "standard-version --release-as patch",
    "release-minor": "standard-version --release-as minor",
    "release-major": "standard-version --release-as major",
    "alpha": "standard-version --prerelease alpha",
    "beta": "standard-version --prerelease beta",
    "rc": "standard-version --prerelease rc",
    "release:first": "standard-version --first-release",
    "release:note": "conventional-github-releaser -p angular "
